													<div class="block-item__code code-item">
														<p class="code-item__main">function ibg(){</p>
														<span class="code-item__descr">$.each($('.ibg'), function(index, val) {</span>
														<span class="code-item__descr"><span class="code-item__descr">if($(this).find('img').length&gt;0){</span></span>
														<span class="code-item__descr"><span class="code-item__descr"><span class="code-item__descr">$(this).css('background-image','url("'+$(this).find('img').attr('src')+'")');</span></span></span>
														<span class="code-item__descr"><span class="code-item__descr">}</span></span>
														<span class="code-item__descr">});</span>
														<p class="code-item__main">}</p>
														<p class="code-item__main">ibg();</p>					
													</div>

