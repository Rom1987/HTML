												<div class="nav-sector__item tab__navitem">Цветной</div>
												<div class="nav-sector__item tab__navitem">Цветной (RGB)</div>
												<div class="nav-sector__item tab__navitem">Цветной (RGBA)</div>
											</div>

											<!-- Тело табов -->
											<div class="sector-block-body tab-block">

												<!-- Активный таб -->
												<div class="sector-block-item tab__item active">

													<!-- Область с визуальным результатом -->
													<div class="block-item-block">
														<div class="block-item-block__value"></div>
													</div>

													<!-- Пояснение с конкретным кодом -->	
													<div class="block-item__label">CSS код:</div>
													<div class="block-item__code code-item">
														<p class="code-item__main">.block{</p>
														<span class="code-item__descr">background: transparent;</span>
														<p class="code-item__main">}</p>
													</div>

													<div class="block-item__or">Или:</div>
													<div class="block-item__code code-item">
														<p class="code-item__main">.block{</p>
														<span class="code-item__descr">background-color: transparent;</span>
														<p class="code-item__main">}</p>
													</div>

												</div>

												<!-- Второй таб -->
												<div class="sector-block-item tab__item">

													<!-- Область с визуальным результатом -->
													<div class="block-item-block">
														<div style="background: #18b5a4;" class="block-item-block__value"></div>
													</div>

													<!-- Пояснение с конкретным кодом -->
													<div class="block-item__label">CSS код:</div>
													<div class="block-item__code code-item">
														<p class="code-item__main">.block{</p>
														<span class="code-item__descr">background: #18b5a4;</span>
														<p class="code-item__main">}</p>
													</div>

													<div class="block-item__or">Или:</div>
													<div class="block-item__code code-item">
														<p class="code-item__main">.block{</p>
														<span class="code-item__descr">background-color: #18b5a4;</span>
														<p class="code-item__main">}</p>
													</div>

												</div>

												<!-- Третий таб -->
												<div class="sector-block-item tab__item">

													<!-- Область с визуальным результатом -->
													<div class="block-item-block">
														<div style="background: rgb(24,181,164);" class="block-item-block__value"></div>
													</div>

													<!-- Пояснение с конкретным кодом -->
													<p class="block-item__text">Кодировщик <a href="http://hex2rgba.devoth.com/" target="_blank" class="block-item__link">из HEX в RGB</a></p>
													<div class="block-item__label">CSS код:</div>
													<div class="block-item__code code-item">
														<p class="code-item__main">.block{</p>
														<span class="code-item__descr">background: rgb(24,181,164);</span>
														<p class="code-item__main">}</p>
													</div>

													<div class="block-item__or">Или:</div>
													<div class="block-item__code code-item">
														<p class="code-item__main">.block{</p>
														<span class="code-item__descr">background-color: rgb(24,181,164);</span>
														<p class="code-item__main">}</p>
													</div>

												</div>

												<!-- Четвертый таб -->
												<div class="sector-block-item tab__item">

													<!-- Область с визуальным результатом -->
													<div class="block-item-block">
														<div style="background: rgba(24,181,164,0.5);" class="block-item-block__value"></div>
													</div>

													<!-- Пояснение с конкретным кодом -->
													<p class="block-item__text">Кодировщик <a href="http://hex2rgba.devoth.com/" target="_blank" class="block-item__link">из HEX в RGBA</a></p>
													<div class="block-item__label">CSS код:</div>
													<div class="block-item__code code-item">
														<p class="code-item__main">.block{</p>
